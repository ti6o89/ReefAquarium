@model IEnumerable<AquariumListingServiceModel>
@{
    ViewData["Title"] = "All Aquariums";
}
<h2>@ViewData["Title"]</h2>
<div class="row">
    @foreach (var aquarium in Model)
    {
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title text-center">
                        <a asp-controller="Aquariums" asp-action="Details" asp-route-id="@aquarium.Id">@aquarium.Title</a>
                    </h3>
                </div>
                <div class="panel-body">
                    <img src="@aquarium.ImageUrl" class="img-responsive">
                    <br />
                    <p class="text-center">@aquarium.Owner</p>
                </div>
            </div>
        </div>
    }
</div>